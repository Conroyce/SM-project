<div class="abs">
  <div class="row response-wrapper">
    <div class="top-pad">    
        <div class="row space-bot-1">
          <div class="col-xs-10">
            <h3>{{data.1.name}} <span class="date">{{data.1.date}}</span></h3>
          </div>
          <div class="col-xs-2">
            <p class="num">{{data.1.num}}</p>
          </div>
        </div> 
      {{#each data}}  
        <div class="row">
          <div class="col-xs-12">
            <p class="question">{{question}}</p>
          </div>
        </div>
        <div class="row">
          <div class="col-xs-12">
            <p>{{answer}}</p>
          </div>
        </div>     
      {{/each}}
    </div> 
    <div class="row bot-pad-show">
    </div> 
  </div>  
</div>